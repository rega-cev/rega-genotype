<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 4.0//EN"
        "http://www.tuckey.org/res/dtds/urlrewrite4.0.dtd">

<urlrewrite>
    <rule>
        <from>/([^/]+)/typingtool/(.*)</from>
        <to>/typingtool/$1</to>
    </rule>
    
    <rule>
        <from>/([^/]+)/typingtool</from>
        <to>/typingtool/$1</to>
    </rule>
    
    <rule>
        <from>^/typingtool/([^/]+)/style/(.*)</from>
        <to>/style/$2</to>
    </rule>
    
     <rule>
        <from>^/typingtool/style</from>
        <to>/style</to>
    </rule>
</urlrewrite>