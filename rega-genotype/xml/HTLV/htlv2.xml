					<taxus name="BAIDU25" />
					<taxus name="BAIDU70" />
					<taxus name="Dub496" />
					<taxus name="Dub991" />
					<taxus name="Mo" />
					<taxus name="HIVAR16" />
					<taxus name="Tyrio80" />
					<taxus name="Belem02" />
					<taxus name="Dub805" />
