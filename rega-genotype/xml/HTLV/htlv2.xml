					<taxus name="BCIH2" />
					<taxus name="PH230PCAM" />
				</cluster>
				<cluster id="a(subtype_c)" name="HTLV-2 - Subtype c (Brazilian Group)">
					<description>HTLV-2 subtype c Brazilian Group</description>
					<taxus name="BEL3948" />
					<taxus name="BEL4076" />
					<taxus name="GTY9272" />
					<taxus name="TYR8668" />
