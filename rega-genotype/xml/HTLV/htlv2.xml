<genotype-analyses>
	<alignment file="htlv2.fasta" trim="true" />
		<clusters>
			<cluster id="d" name="HTLV-2 - Subtype d">
				<taxus name="pygmy2" />
			</cluster>
			
			<cluster id="a" name="HTLV-2 - Subtype a">
				<cluster id="a(subtype_a)" name="HTLV-2 - Subtype a">
					<taxus name="ghkt" />
