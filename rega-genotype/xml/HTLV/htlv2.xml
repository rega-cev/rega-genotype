				</cluster>
			</cluster>
			
			<cluster id="b" name="HTLV-2 - Subtype b">
				<taxus name="RC" />
				<taxus name="DP" />
				<taxus name="IIb" />
				<taxus name="Gab1080FC" />
				<taxus name="IOG" />
				<taxus name="IOV" />
				<taxus name="GAB" />
				<taxus name="Pyl36" />
				<taxus name="Pyl115" />
				<taxus name="Pygmy230102" />
				<taxus name="Pygmy330110" />
				<taxus name="BDAR2" />
				<taxus name="BDAR3" />
				<taxus name="SP1" />
				<taxus name="SP2" />
				<taxus name="NRA" />
				<taxus name="G2" />
				<taxus name="PortNn" />
				<taxus name="wyu1" />
				<taxus name="wyu2" />
				<taxus name="PYGCAM1" />
